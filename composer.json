{
    "name": "amirezaeb/heroqr", 
    "description": "A Powerful QR Code Management Library For PHP",
    "keywords": ["AmirezaEb", "qrcode", "qr", "management", "code", "php"],
    "homepage": "https://github.com/AmirezaEb/HeroQR",
    "version": "1.0.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Amirreza",
            "email": "aabrahimi1718@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "HeroQR\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HeroQR\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "endroid/qr-code": "^6.0",
        "setasign/fpdf": "^1.8",
        "giggsey/libphonenumber-for-php": "^8.13",
        "illuminate/support": "^11.35"
    },
    "require-dev": {          
        "phpunit/phpunit": "^11.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "HeroQR\\Providers\\HeroQRServiceProvider"
            ]
        }
    }
}
